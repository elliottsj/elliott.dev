  "description": "My personal website and blog",
  "scripts": {
    "?build": "Compile the application for production deployment to the directory \".next/\".",
    "build": "next build",
    "?dev": "Launch the application in development mode.",
    "dev": "next",
    "?dev:debug": "Launch the application in development mode, with debugging enabled.",
    "dev:debug": "cross-env NODE_OPTIONS=\"--inspect-brk\" next",
    "?export": "Export the application for production deployment.",
    "export": "npm run build && next export",
    "?format:check": "Check that all source files are formatted correctly with Prettier.",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx}\"",
    "?format:write": "Format all incorrectly-formatted source files and writes them to disk.",
    "format:write": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "?info": "Print info about npm scripts",
    "info": "npm-scripts-info",
    "?jest": "Run Jest tests.",
    "jest": "jest",
    "?jest:watch": "Run Jest tests in watch mode.",
    "jest:watch": "jest --watch",
    "?lint": "Lint source files.",
    "lint": "next lint",
    "?start": "Start the application in production mode. Must run \"build\" first.",
    "start": "next start",
    "?test": "Run all tests",
    "test": "npm-run-all --sequential format:check lint jest build"
  },
